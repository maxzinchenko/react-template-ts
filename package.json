    "html-webpack-plugin": "4.5.1",
    "husky": "4.3.8",
    "jest": "26.6.3",
    "sass-loader": "10.1.1",
    "style-loader": "2.0.0",
    "ts-loader": "8.0.14",
    "typescript": "4.1.3",
